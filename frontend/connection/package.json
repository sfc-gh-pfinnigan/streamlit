{
  "name": "@streamlit/connection",
  "version": "1.0.0",
  "private": true,
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:src && yarn build:dev",
    "build:src": "env NODE_ENV=development ./node_modules/.bin/babel --root-mode=upward --out-dir dist --source-maps --extensions \".js,.mjs,.jsx,.ts,.tsx\" src",
    "build:dev": "tsc -p ./tsconfig.json && tsc-alias -p ./tsconfig.json",
    "typecheck": "tsc",
    "test": "env NODE_OPTIONS=--max_old_space_size=8192 jest --env=../jest/jsdom-polyfill-env.js --runTestsByPath ${TESTPATH}",
    "testWatch": "env NODE_OPTIONS=--max_old_space_size=8192 jest --watchAll --env=../jest/jsdom-polyfill-env.js --runTestsByPath ${TESTPATH}",
    "test:coverage": "env NODE_OPTIONS=--max_old_space_size=8192 jest --env=../jest/jsdom-polyfill-env.js --coverage --watchAll false --watch false ./",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --max-warnings 0 src"
  },
  "dependencies": {
    "@emotion/styled": "^11.10.5",
    "@streamlit/proto": "1.0.0",
    "@streamlit/utils": "1.0.0",
    "axios": "^1.7.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@emotion/babel-plugin": "^11.7.2",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.4.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "babel-jest": "^27.4.3",
    "babel-plugin-emotion": "^11.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint-plugin-streamlit-custom": "file:../eslint-plugin-streamlit-custom",
    "jest": "^27.4.3",
    "jest-environment-jsdom": "^27.4.3",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "2.6.7",
    "tsc-alias": "^1.8.5"
  },
  "resolutions": {
    "axios": "^0.27.2"
  }
}
